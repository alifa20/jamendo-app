{
  "private": true,
  "name": "@jamendo/app-example",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "lint": "eslint --ext js,ts,tsx .",
    "typecheck": "tsc --noEmit",
    "test": "echo 'Maestro tests skipped - run test:maestro to execute E2E tests'",
    "test:maestro": "maestro test .maestro/flows",
    "test:single": "maestro test",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas-build-post-install": "pnpm run -w build:example"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@jamendo/components": "workspace:*",
    "@jamendo/feature-home": "workspace:*",
    "@jamendo/rtk-services": "workspace:*",
    "@jamendo/ui": "workspace:*",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "^1.8.3",
    "expo": "~54.0.6",
    "expo-audio": "~1.0.14",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-router": "~6.0.3",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-web": "~0.21.0",
    "react-redux": "^9.2.0",
    "recyclerlistview": "^4.2.3"
  },
  "devDependencies": {
    "@expo/metro": "^0.1.1",
    "@expo/metro-runtime": "~6.1.2",
    "@jamendo/eslint-config": "workspace:*",
    "@reduxjs/toolkit": "^2.10.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "eslint": "^8.57.0",
    "typescript": "~5.9.2"
  },
  "eslintConfig": {
    "extends": "@jamendo/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared",
      "*.d.ts"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
